  "engines": {
    "node": ">=10.5.0",
    "npm": ">=6.0.0"
  },
  "lint-staged": {
    "units/**/*.js": [
      "eslint --fix",
      "prettier-eslint --write",
      "git add"
    ],
    "units/**/*.jsx": [
      "eslint --fix",
      "prettier-eslint --write",
      "git add"
    ]
  },
  "name": "UniversalRelayBoilerplate",
  "prettier": {
    "bracketSpacing": true,
    "printWidth": 100,
    "parser": "flow",
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all",
    "useTabs": false
  },
  "scripts": {
    "build-bump-version": "./node_modules/.bin/babel-node ./units/urb-base-tools/buildBumpVersion.js",
    "build-deployment": "npm run build-relay && npm run build-server && npm run build-webpack",
    "build-relay": "./node_modules/.bin/babel-node ./units/urb-base-tools/buildUnits.js && ./node_modules/.bin/relay-compiler --src ./units/ --extensions=js jsx --schema schema.graphql",
    "build-server": "export NODE_ENV=production && rm -rf deployment/* && rsync -avm --exclude='*.js' --exclude='*.jsx' units ./deployment/ && ./node_modules/.bin/babel -s true --retain-lines -d ./deployment/units/ ./units && cp ./package.json ./deployment && cp ./yarn.lock ./deployment",
    "build-units": "./node_modules/.bin/babel-node ./units/urb-base-tools/buildUnits.js",
    "build-webpack": "export NODE_ENV=production && rm -rf deployment/units/_configuration/urb-base-server/public_files/assets/* &&  ./node_modules/.bin/webpack --mode production -p",
    "deb": "./node_modules/.bin/concurrently --names \"RELAY,WEB,NODE\" -c \"bgMagenta.white,bgBlue.white,bgRed.white\" --kill-others \"npm run dev-relay\" \"npm run dev-webpack\" \"npm run dev-server-debug\" ",
    "dev": "./node_modules/.bin/concurrently --names \"RELAY,WEB,NODE\" -c \"bgMagenta.white,bgBlue.white,bgGreen.white\" --kill-others \"npm run dev-relay\" \"npm run dev-webpack\" \"npm run dev-server\" ",
    "dev-prod": "export NODE_ENV=production && ./node_modules/.bin/babel-node ./units/urb-base-server/server.js",
    "dev-relay": "./node_modules/.bin/relay-compiler --src ./units/ --extensions=js jsx --schema schema.graphql --watch",
    "dev-reset": "watchman watch-del-all &&  rm -rf $TMPDIR/react-* && rm -rf ~/.node-gyp  && (cd units && find . -type d -name '__generated__' | xargs rm -r)",
    "dev-server": "export NODE_ENV=development && ./node_modules/.bin/babel-watch -e js,jsx ./units/urb-base-server/server.js",
    "dev-server-debug": "export NODE_ENV=development && ./node_modules/.bin/babel-watch --inspect -e js,jsx ./units/urb-base-server/server.js",
    "dev-webpack": "./node_modules/.bin/webpack-dev-server --mode development --hot --inline --no-info --colors",
    "postinstall": "node ./units/urb-example-ensayo-webapp/fix-react-rte.js",
    "precommit": "yarn run flow && lint-staged",
    "setup-database": "./node_modules/.bin/babel-node ./units/urb-base-tools/setupDatabase.js",
    "setup-local": "cp ./units/_configuration/default.env ./.env",
    "setup-local-cut-version": "cp ./units/_configuration/default.env ./.env",
    "update-ip": "./node_modules/.bin/babel-node ./units/urb-base-tools/updateIP.js"
  },
  "version": "1.0.0"
}
