  "name": "rebar",
  "prettier": {},
  "scripts": {
    "build-bump-version": "./node_modules/.bin/babel-node ./units/rb-base-tools/buildBumpVersion.js",
    "build-deployment": "rm -rf deployment && mkdir deployment && yarn build-units && yarn && yarn build-bump-version && yarn build-relay && yarn build-server && yarn build-webpack",
    "build-relay": "./node_modules/.bin/babel-node ./units/rb-base-tools/buildUnits.js && ./node_modules/.bin/relay-compiler --src ./units/ --extensions=js jsx --schema schema.graphql",
    "build-server": "export NODE_ENV=production && rm -rf deployment/* && rsync -avm --exclude='*.js' --exclude='*.jsx' --exclude='*.test.js' units ./deployment/ && ./node_modules/.bin/babel -s true --retain-lines  --ignore *.test.js -d ./deployment/units/ ./units && cp ./package.json ./deployment && cp ./yarn.lock ./deployment",
    "build-units": "./node_modules/.bin/babel-node ./units/rb-base-tools/buildUnits.js",
    "build-webpack": "export NODE_ENV=production && rm -rf deployment/units/_configuration/rb-base-server/public_files/assets/* &&  ./node_modules/.bin/webpack --mode production -p",
    "deb": "./node_modules/.bin/concurrently --names \"RELAY,WEB,NODE\" -c \"bgYellow.black,bgBlue.black,bgGreen.black\" --kill-others \"npm run dev-relay\" \"npm run dev-webpack\" \"npm run dev-server-debug\" ",
    "dev": "./node_modules/.bin/concurrently --names \"RELAY,WEB,NODE\" -c \"bgYellow.black,bgBlue.black,bgGreen.black\" --kill-others \"npm run dev-relay\" \"npm run dev-webpack\" \"npm run dev-server\" ",
    "dev-m": "./node_modules/.bin/concurrently --names \"NODE,TUNNEL\" -c \"bgGreen.black,bgRed.yellow\" --kill-others \"npm run dev-server\" \"npm run dev-tunnel\" ",
    "dev-prod": "export NODE_ENV=production && ./node_modules/.bin/babel-node ./units/rb-base-server/server.js",
    "dev-relay": "./node_modules/.bin/relay-compiler --src ./units/ --extensions=js jsx --schema schema.graphql --watch",
    "dev-reset": "watchman watch-del-all &&  rm -rf $TMPDIR/react-* && rm -rf ~/.node-gyp  && (cd units && find . -type d -name '__generated__' | xargs rm -r)",
    "dev-server": "export NODE_ENV=development && ./node_modules/.bin/babel-watch -e js,jsx ./units/rb-base-server/server.js",
    "dev-server-debug": "export NODE_ENV=development && ./node_modules/.bin/babel-watch --inspect -e js,jsx ./units/rb-base-server/server.js",
    "dev-server-nowatch": "export NODE_ENV=development && ./node_modules/.bin/babel-node  ./units/rb-base-server/server.js",
    "dev-t": "./node_modules/.bin/concurrently --names \"RELAY,WEB,NODE,TUNNEL\" -c \"bgYellow.black,bgBlue.black,bgGreen.black,bgRed.yellow\" --kill-others \"npm run dev-relay\" \"npm run dev-webpack\" \"npm run dev-server\" \"npm run dev-tunnel\" ",
    "dev-tunnel": "node ./units/rb-devtunnel/tunnel",
    "dev-webpack": "./node_modules/.bin/webpack-dev-server --mode development --hot --inline --no-info --colors",
    "flow": "./node_modules/.bin/flow",
    "postinstall": "node ./units/rb-example-ensayo-webapp/fix-react-rte.js",
    "setup-database": "./node_modules/.bin/babel-node ./units/rb-appbase-server/setupDatabase.js",
    "setup-local": "cp ./units/_configuration/default.env ./.env",
    "setup-local-cut-version": "cp ./units/_configuration/default.env ./.env",
    "test": "jest",
    "update-ip": "./node_modules/.bin/babel-node ./units/rb-base-tools/updateIP.js"
  },
  "version": "4.2.1"
}