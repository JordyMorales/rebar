    "serialize-javascript": "1.5.0",
    "winston": "2.4.2"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.5",
    "babel-loader": "7.1.4",
    "babel-plugin-dynamic-import-node": "2.0.0",
    "babel-plugin-dynamic-import-webpack": "1.0.2",
    "babel-plugin-flow-react-proptypes": "24.0.1",
    "babel-plugin-react-transform": "3.0.0",
    "babel-plugin-relay": "1.6.0",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.13",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-native-stage-0": "1.0.1",
    "babel-preset-stage-3": "6.24.1",
    "babel-watch": "2.0.7",
    "concurrently": "3.6.0",
    "eslint": "5.0.1",
    "eslint-config-prettier": "2.9.0",
    "eslint-loader": "2.0.0",
    "eslint-config-react-app": "2.1.0",
    "eslint-plugin-flowtype": "2.49.3",
    "eslint-plugin-import": "2.13.0",
    "eslint-plugin-import-order-autofix": "0.8.3",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-prettier": "2.6.1",
    "eslint-plugin-react": "7.10.0",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "flow-bin": "0.75.0",
    "husky": "0.14.3",
    "lint-staged": "7.2.0",
    "prettier": "1.13.7",
    "prettier-eslint": "8.8.2",
    "prettier-eslint-cli": "4.7.1",
    "recursive-readdir-sync": "1.0.6"
  },
  "prettier": {
    "bracketSpacing": true,
    "printWidth": 100,
    "parser": "flow",
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all",
    "useTabs": false
  },
  "scripts": {
    "build-deployment": "npm run build-relay && npm run build-server && npm run build-webpack",
    "build-relay":
      "./node_modules/.bin/babel-node ./units/urb-base-tools/buildUnits.js && ./node_modules/.bin/relay-compiler --src ./units/ --extensions=js jsx --schema schema.graphql",
    "build-server":
      "export NODE_ENV=production && rm -rf deployment/* && rsync -avm --exclude='*.js' --exclude='*.jsx' units ./deployment/ && ./node_modules/.bin/babel -s true --retain-lines -d ./deployment/units/ ./units && cp ./package.json ./deployment && cp ./yarn.lock ./deployment",
    "dev":
      "./node_modules/.bin/concurrently --names \"RELAY,WEB,NODE\" -c \"bgMagenta.white,bgBlue.white,bgGreen.white\" --kill-others \"npm run dev-relay\" \"npm run dev-webpack\" \"npm run dev-server\" ",
    "deb":
      "./node_modules/.bin/concurrently --names \"RELAY,WEB,NODE\" -c \"bgMagenta.white,bgBlue.white,bgRed.white\" --kill-others \"npm run dev-relay\" \"npm run dev-webpack\" \"npm run dev-server-debug\" ",
    "dev-reset":
      "watchman watch-del-all &&  rm -rf $TMPDIR/react-* && rm -rf ~/.node-gyp  && (cd units && find . -type d -name '__generated__' | xargs rm -r)",
    "dev-prod":
      "export NODE_ENV=production && ./node_modules/.bin/babel-node ./units/urb-base-server/server.js",
    "dev-server":
      "export NODE_ENV=development && ./node_modules/.bin/babel-watch -e js,jsx ./units/urb-base-server/server.js",
    "dev-server-debug":
      "export NODE_ENV=development && ./node_modules/.bin/babel-watch --inspect -e js,jsx ./units/urb-base-server/server.js",
    "setup-database": "./node_modules/.bin/babel-node ./units/urb-base-tools/setupDatabase.js"
  }
}
