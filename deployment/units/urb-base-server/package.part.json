  "prettier": {
    "bracketSpacing": true,
    "printWidth": 100,
    "parser": "flow",
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all",
    "useTabs": false
  },
  "scripts": {
    "build-deployment": "npm run build-relay && npm run build-server && npm run build-webpack",
    "build-relay":
      "./node_modules/.bin/babel-node ./units/urb-base-tools/build-units.js && ./node_modules/.bin/babel-node ./units/urb-base-server/build-relay-schema.js && ./node_modules/.bin/relay-compiler --src ./units/ --extensions=js jsx --schema units/_configuration/urb-base-server/graphql/schema.graphql",
    "build-server":
      "export NODE_ENV=production && rm -rf deployment/* && cp -r ./units/_configuration ./deployment/units && ./node_modules/.bin/babel -s true --retain-lines -d ./deployment/units/ ./units && rsync -avm --include='*.ejs' --include='*.json' -f 'hide,! */' units ./deployment/ && rsync -avm --include='*.graphql.js' -f 'hide,! */' units ./deployment/ && cp ./package.json ./deployment && cp ./yarn.lock ./deployment",
    "build-webpack":
      "export NODE_ENV=production && rm -rf deployment/units/_configuration/urb-base-server/public_files/assets/* &&  ./node_modules/.bin/webpack -p --output-filename client.js",
    "dev":
      "./node_modules/.bin/concurrently --kill-others \"npm run dev-relay\" \"npm run dev-webpack\" \"npm run dev-server\" ",
    "dev-reset":
      "watchman watch-del-all &&  rm -rf $TMPDIR/react-* && rm -rf ~/.node-gyp  && (cd units && find . -type d -name '__generated__' | xargs rm -r)",
    "dev-prod":
      "export NODE_ENV=production && ./node_modules/.bin/babel-node ./units/urb-base-server/server.js",
    "dev-server":
      "export NODE_ENV=development && ./node_modules/.bin/babel-watch -e js,jsx ./units/urb-base-server/server.js",
    "dev-webpack": "./node_modules/.bin/webpack-dev-server --hot --inline --no-info --colors",
    "dev-relay":
      "./node_modules/.bin/relay-compiler --src ./units/ --extensions=js jsx --schema units/_configuration/urb-base-server/graphql/schema.graphql --watch",
    "setup-database": "./node_modules/.bin/babel-node ./units/urb-base-tools/setup-database.js",
    "update-default-persister":
      "./node_modules/.bin/babel-node ./units/urb-base-tools/update-default-persister.js"
  }
}
